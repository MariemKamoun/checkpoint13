    "import time\n",
    "t=int(input(\"length of the countdown in seconds\"))\n",
    "def countdown(t):\n",
    "    while t>0:\n",
    "        timeformat=divmod(t,60)\n",
    "        print(timeformat,end='\\r')\n",
    "        time.sleep(1)\n",
    "        t=t-1\n",
    "    print (\"Fire in the hole\")\n",
    "countdown(t)\n"
   ]
